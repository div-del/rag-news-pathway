{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/",
            "destination": "/landing.html"
        },
        {
            "source": "/app",
            "destination": "/app.html"
        },
        {
            "source": "/onboarding",
            "destination": "/onboarding.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}